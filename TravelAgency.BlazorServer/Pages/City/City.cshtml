@model TravelAgency.Domain.Models.City;


@if (Model != null)
{
    <tr class="city">
        <td>
            @Model.Name
        </td>
        <td>
            @if (Model.Country != null)
            {
                @Model.Country.Name
            }
        </td>
        <td>

            <div id="deleteForm">
                <a asp-action="Edit" asp-route-id="@Model.CityId">Edit</a>
                <a asp-action="Delete" asp-route-id="@Model.CityId">Delete</a>
            </div>

        </td>

    </tr>
}